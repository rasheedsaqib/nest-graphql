{
  "name": "nest-starter",
  "version": "0.0.1",
  "description": "",
  "author": "Saqib Rasheed",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format:eslint": "eslint src --ext .ts --fix",
    "format:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format": "yarn run format:eslint && yarn run format:prettier",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint:lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:type": "tsc",
    "lint": "yarn run lint:lint && yarn run lint:type",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config src/e2e/jest-e2e.json",
    "postinstall": "npx husky install"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/sequelize": "^10.0.0",
    "argon2": "^0.31.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "graphql": "^16.8.1",
    "mysql2": "^3.6.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sequelize": "^6.37.1",
    "sequelize-typescript": "^2.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.6.2",
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.8",
    "@types/node": "20.11.19",
    "@types/passport-jwt": "^4.0.1",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "37.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-unicorn": "49.0.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "source-map-support": "^0.5.20",
    "supertest": "6.3.4",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2"
  }
}
